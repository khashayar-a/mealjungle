          RESTAURANT
        *************
    -->
    <div class="tab-pane fade active in" id="restaurant">
        <form class="form-horizontal" role="form">
            <div class="form-group">
                <div class="forlabel">
                    Name of establishment*
                </div>
                <input id="resName" type="name" class="form-control" id="resName" placeholder="eg. Harry's Bar" >
            </div>
            <div class="form-group">
                <div class="forlabel">
                    Type of establishment*
                </div>
                <div class="dropdown">
                    <button class="btn btn-default dropdown-toggle" type="button" id="dropdown-choices" data-toggle="dropdown">
                        <p>Select one or several options from the dropdown menu<span class="caret"></span></p>
                    </button>
                    <ul class="dropdown-menu" id="btn-resTypeChoice" role="resType" aria-labelledby="resType">
                        <li data-value="restaurant"><a>Restaurant</a></li>
                        <li data-value="cafe"><a>Cafe</a></li>
                        <li data-value="pizzeria"><a>Pizzeria</a></li>
                        <li data-value="bar"><a>Bar</a></li>
                        <li data-value="fast_food"><a>Fast food</a></li>
                    </ul>
                </div>

                <div class=" choices resType">
                </div>
            </div>
            <div class="form-group">
                <div class="forlabel">
                    Address*
                </div>
                <input id="resAddress" type="address" class="form-control" placeholder="eg. Storgatan 3, Gothenburg, Sweden">
                <div id="map-canvas"></div>
            </div>
            <div class="form-group">
                <div class="forlabel">
                    Phone number
                </div>
                <input type="phone" class="form-control" id="resPhone" placeholder="eg. +46 707 17 93 40">
            </div>
            <div class="form-group">
                <div class="forlabel">
                    Email
                </div>
                <input type="email" class="form-control" id="resEmail" placeholder="eg. harry@harrysbar.com">
            </div>
            <div class="form-group">
                <div class="forlabel">
                    Website
                </div>
                <input type="website" class="form-control" id="resWebsite" placeholder="eg. www.harrysbar.com">
            </div>
            <div class="form-group">
                <div class="forlabel">
                    Description
                </div>
                <textarea id="resDescription" class="form-control " placeholder="eg. The best place in the world!"></textarea>
            </div>
            <div class="form-group">
                <div class="forlabel">
                    Cuisine
                </div>
                <div class="dropdown">
                    <button class="btn btn-default dropdown-toggle" type="button" id="dropdown-choices" data-toggle="dropdown">
                        <p>Select one or several options from the dropdown menu<span class="caret"></span></p>
                    </button>
                    <ul class="dropdown-menu" id="btn-resCuisineChoice" role="resCuisine" aria-labelledby="resCuisine">
                        <li data-value="swedish"><a>Swedish</a></li>
                        <li data-value="persian"><a>Persian</a></li>
                        <li data-value="italian"><a>Italian</a></li>
                        <li data-value="indian"><a>Indian</a></li>
                        <li data-value="french"><a>French</a></li>
                        <li data-value="asian"><a>Asian</a></li>
                    </ul>
                </div>
                
                <div class=" choices resCuisine">
                </div>
            </div>
            <div class="form-group">
                <div class="forlabel">
                    Food
                </div>
                <div class="dropdown">
                    <button class="btn btn-default dropdown-toggle" type="button" id="dropdown-choices" data-toggle="dropdown">
                        <p>Select one or several options from the dropdown menu<span class="caret"></span></p>
                    </button>
                    <ul class="dropdown-menu" id="btn-resFoodChoice" role="resFood" aria-labelledby="resFood">
                        <li data-value="meat"><a>Meat</a></li>
                        <li data-value="salad"><a>Salad</a></li>
                        <li data-value="vegetarian"><a>Vegetarian</a></li>
                        <li data-value="vegan"><a>Vegan</a></li>
                        <li data-value="sea-food"><a>Sea Food</a></li>
                        <li data-value="dessert"><a>Dessert</a></li>
                    </ul>
                </div>
                
                <div class=" choices resFood">
                </div>
            </div>
            <div class="form-group">
                <div class="forlabel">
                    Meal time
                </div>
                <div id="resMealTime" class="btn-group btn-group-justified " data-toggle="buttons">
                    <label class="btn btn-default">
                        <input id="DDD" type="checkbox" value="breakfast" /> Breakfast
                    </label> 
                    <label class="btn btn-default">
                        <input type="checkbox" value="lunch" /> Lunch
                    </label> 
                    <label class="btn btn-default">
                        <input type="checkbox" value="brunch" /> Brunch
                    </label> 
                    <label class="btn btn-default">
                        <input type="checkbox" value="dinner" /> Dinner
                    </label> 
                </div>
            </div>
            <div class="form-group">
                <div class="forlabel">
                    Price range
                </div>
                <div id="resPrice" class="btn-group btn-group-justified " data-toggle="buttons">
                    <label class="btn btn-default">
                        <input type="radio" value="low" /> Low
                    </label> 
                    <label class="btn btn-default">
                        <input type="radio" value="medium" /> Medium
                    </label> 
                    <label class="btn btn-default">
                        <input type="radio" value="high" /> High
                    </label> 
                </div>
            </div>
            <div class="form-group">
                <a  id="btn-save" type="button" class="btn btn-info">Save</a>
            </div>
        </form>
    </div>
    
